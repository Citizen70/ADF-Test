{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ABRA-Demo"
		},
		"ControlTable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ControlTable'"
		},
		"Dynamics1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Dynamics1'"
		},
		"DynamicsCrm1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DynamicsCrm1'"
		},
		"dl_StorageLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dl_StorageLinkedService'"
		},
		"Dynamics1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "crmdev@mei-avivim.co.il"
		},
		"DynamicsCrm1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "crmdev@mei-avivim.co.il"
		},
		"dl_StorageLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://abrademostorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ABRA-Demo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ControlTable_connectionString": {
						"value": "[parameters('ControlTable_connectionString')]"
					},
					"Dynamics1_password": {
						"value": "[parameters('Dynamics1_password')]"
					},
					"DynamicsCrm1_password": {
						"value": "[parameters('DynamicsCrm1_password')]"
					},
					"dl_StorageLinkedService_accountKey": {
						"value": "[parameters('dl_StorageLinkedService_accountKey')]"
					},
					"Dynamics1_properties_typeProperties_username": {
						"value": "[parameters('Dynamics1_properties_typeProperties_username')]"
					},
					"DynamicsCrm1_properties_typeProperties_username": {
						"value": "[parameters('DynamicsCrm1_properties_typeProperties_username')]"
					},
					"dl_StorageLinkedService_properties_typeProperties_url": {
						"value": "[parameters('dl_StorageLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}